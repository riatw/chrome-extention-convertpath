{
   "manifest_version": 2,
   "name": "Convert Path",
   "description": "",
   "version": "1.0",
   "minimum_chrome_version": "29",
   "icons": {
      "128": "images/icon/icon_128.png"
   },
   "background": {
      "page": "background.html",
      "persistent": true
   },
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "clipboardRead",
    "tabs"
  ],
  "content_scripts": [
      {
          "matches": [
              "https://www.chatwork.com/*",
              "https://*.cybozu.com/*",
              "https://cybozulive.com/*",
              "https://*.backlog.jp/*"
          ],
          "js": [
              "js/contents.js"
          ]
      }
  ],
  "commands": {
    "convert-mac-to-win": {
       "description": "convert-mac-to-win",
       "global": true,
       "suggested_key": {
          "default": "Ctrl+Shift+M"
       }
    },
    "convert-win-to-mac": {
       "description": "convert-win-to-mac",
       "global": true,
       "suggested_key": {
          "default": "Ctrl+Shift+W"
       }
    }
  }
}